<?xml version="1.0" encoding="utf-8"?>
<s:DefaultGridHeaderRenderer xmlns:fx="http://ns.adobe.com/mxml/2009" 
				xmlns:s="library://ns.adobe.com/flex/spark" 
				xmlns:mx="library://ns.adobe.com/flex/mx">
	
	<fx:Script>
		<![CDATA[
			protected function clickHandler(event:MouseEvent):void
			{
				this.parentApplication.bodyStack.welcomeDashboard.serviceHealthComponent.previousWeek();
			}
		]]>
	</fx:Script>
	
	<s:Rect height="100%" width="100%">
		<s:fill>
			<s:LinearGradient rotation="90">
				<s:GradientEntry color="#ffffff"/>
				<s:GradientEntry color="#cccccc"/>
			</s:LinearGradient>
		</s:fill>
	</s:Rect>
	<mx:VBox width="100%" height="100%" verticalAlign="middle" horizontalAlign="center" click="clickHandler(event)">
		<s:Image source="/com/momentumsi/c9/assets/leftArrows.gif" width="15" height="14"/>
	</mx:VBox>
</s:DefaultGridHeaderRenderer>
